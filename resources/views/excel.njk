{% extends 'layout_app' %}

{% block content %}
  <div class="container">
    <div class="row">
      <div class="col-md-12">

        <!-- Navigation-->
        <ul class="breadcrumb">
          <li><a href="#">Dashboard</a></li>
          <li class="active">Import Excel</li>
        </ul>
        <!-- End Navigation-->

        <!-- content-->
        <div class="panel panel-default">
          <div class="panel-heading">
            <h2 class="panel-title">People</h2>
          </div>
          <div class="panel-body">
              {{ form.open({action: '', files: true }) }}
              {{ csrfField }}

              <div class="input-group">
                <span class="input-group-addon">Import</span>
                <input id="msg" type="file" class="form-control" name="file">
              </div>
              <div class="input-group">
                {{ form.submit('Upload') }}
              </div>
              {{ form.close() }}
          </div>
        </div>
        <!-- End content-->


      <!-- end of col md 12-->
      </div>
    <!-- end of row-->
    </div>
  <!-- end of container-->
  </div>
{% endblock %}

{% block css %}
  <link href="/css/datatables/dataTables.bootstrap.min.css" rel="stylesheet">
{% endblock %}

{% block script %}
  <script src="/js/datatables/jquery.dataTables.min.js"></script>
  <script src="/js/datatables/dataTables.bootstrap.min.js"></script>
{% endblock %}